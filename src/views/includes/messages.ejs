<% if(erros.length > 0) { %>
    <div class="row">
        <% erros.forEach(erro => { %>
            <div class="col-12">
                <div class="alert alert-danger">
                    <%= erro %> 
                </div>
            </div>
        <% }) %> 
    </div>

<% } %> 

<% if(success.length > 0) { %>
    <div class="row">
        <div class="col-12">
            <div class="alert alert-success">
                <%= success %> 
            </div>
        </div>
    </div>
<% } %> 